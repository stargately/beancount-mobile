{
  "expo": {
    "version": "1.20251212.37",
    "name": "Beancount",
    "scheme": "beancount",
    "slug": "beancount",
    "platforms": ["ios", "android"],
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/eef0c5cf-1a6f-4566-a6a6-e03821db7f8d"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.beancount.ios",
      "userInterfaceStyle": "automatic",
      "infoPlist": {
        "NSCalendarsUsageDescription": false,
        "NSCameraUsageDescription": false,
        "NSContactsUsageDescription": false,
        "NSLocationWhenInUseUsageDescription": false,
        "NSLocationAlwaysUsageDescription": false,
        "NSLocationAlwaysAndWhenInUseUsageDescription": false,
        "NSMicrophoneUsageDescription": false,
        "NSMotionUsageDescription": false,
        "NSPhotoLibraryUsageDescription": false,
        "NSPhotoLibraryAddUsageDescription": false,
        "NSFaceIDUsageDescription": false,
        "ITSAppUsesNonExemptEncryption": false
      },
      "buildNumber": "37"
    },
    "android": {
      "versionCode": 37,
      "package": "io.beancount.android",
      "permissions": ["com.google.android.gms.permission.AD_ID"],
      "edgeToEdgeEnabled": true
    },
    "notification": {
      "icon": "./src/assets/images/icon96.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": ["./src/assets/fonts/SpaceMono-Regular.ttf"]
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/icon.png",
          "resizeMode": "contain",
          "imageWidth": 200,
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-asset",
        {
          "assets": ["./src/assets"]
        }
      ],
      "expo-localization",
      "sentry-expo",
      "expo-router",
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "eef0c5cf-1a6f-4566-a6a6-e03821db7f8d"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
