{
  "expo": {
    "version": "0.8.0",
    "name": "Beancount",
    "slug": "beancount",
    "scheme": "beancount",
    "privacy": "public",
    "platforms": ["ios", "android", "web"],
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.beancount.ios",
      "userInterfaceStyle": "automatic",
      "infoPlist": {
        "NSCalendarsUsageDescription": false,
        "NSCameraUsageDescription": false,
        "NSContactsUsageDescription": false,
        "NSLocationWhenInUseUsageDescription": false,
        "NSLocationAlwaysUsageDescription": false,
        "NSLocationAlwaysAndWhenInUseUsageDescription": false,
        "NSMicrophoneUsageDescription": false,
        "NSMotionUsageDescription": false,
        "NSPhotoLibraryUsageDescription": false,
        "NSPhotoLibraryAddUsageDescription": false,
        "NSFaceIDUsageDescription": false
      }
    },
    "android": {
      "versionCode": 7,
      "package": "io.beancount.android",
      "permissions": []
    },
    "web": {
      "bundler": "metro"
    },
    "notification": {
      "icon": "./src/assets/images/icon96.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": ["./src/assets/fonts/SpaceMono-Regular.ttf"]
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/splash.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-asset",
        {
          "assets": ["./src/assets"]
        }
      ],
      "expo-localization",
      [
        "expo-web-browser",
        {
          "experimentalLauncherActivity": true
        }
      ],
      "sentry-expo",
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}
