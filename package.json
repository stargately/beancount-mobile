{
  "name": "mobile-beancount",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "_note": "npm run lint && npm run tsc-test && ",
    "test": "npm run lint && npm run schema && npm run typecheck",
    "jest": "jest",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit && eslint src --quiet --cache --fix --ext .js,.tsx,.ts ",
    "prettier": "prettier . --cache --write",
    "prettier:check": "prettier . --cache --check",
    "postinstall": "cp -f patch-jest.js ./node_modules/jest-expo/bin/jest.js && npm run schema",
    "schema": "npm run schema:download && npm run schema:generate",
    "schema:download": "npx apollo@2.30.2 schema:download --endpoint=https://beancount.io/api-gateway/ schema.json",
    "schema:generate": "npx apollo@2.30.2 client:codegen --no-addTypename --target=typescript --localSchemaFile=./schema.json",
    "prepare": "husky"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "^.+\\.tsx?$": [
        "babel-jest",
        {
          "configFile": "./babel.config.js"
        }
      ]
    },
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": {
          "jsx": "react"
        }
      }
    }
  },
  "dependencies": {
    "@ant-design/icons-react-native": "^2.3.2",
    "@ant-design/react-native": "^5.4.1",
    "@apollo/client": "3.3.12",
    "@callstack/react-theme-provider": "^3.0.9",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-community/async-storage": "~1.12.0",
    "@react-native-community/cameraroll": "^4.0.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-community/picker": "1.6.6",
    "@react-native-community/segmented-control": "2.2.1",
    "@react-native-community/slider": "4.5.6",
    "@react-native-community/viewpager": "5.0.11",
    "@react-native-picker/picker": "2.11.0",
    "@react-navigation/bottom-tabs": "^6.x",
    "@react-navigation/native": "^6.x",
    "@react-navigation/stack": "^6.x",
    "@sentry/react": "^5.30.0",
    "@sentry/react-native": "~6.10.0",
    "@types/deep-extend": "^0.4.31",
    "@types/i18n-js": "^3.8.0",
    "@types/lodash": "^4.14.168",
    "@types/react-navigation": "^2.13.10",
    "@types/react-redux": "7.1.34",
    "@types/use-subscription": "^1.0.0",
    "@yuyongmao/react-native-chart-kit": "^1.0.0",
    "axios": "^0.21.1",
    "buffer": "^6.0.3",
    "currency-icons": "^1.0.14",
    "deep-extend": "^0.6.0",
    "expo": "^53.0.0",
    "expo-analytics": "^1.0.13",
    "expo-asset": "~11.1.5",
    "expo-constants": "~17.1.6",
    "expo-contacts": "~14.2.4",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-localization": "~16.1.5",
    "expo-mail-composer": "~14.1.4",
    "expo-mixpanel-analytics": "0.0.7",
    "expo-notifications": "~0.31.2",
    "expo-sms": "~13.1.4",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "expo-store-review": "~8.1.5",
    "expo-web-browser": "~14.1.6",
    "fbemitter": "^2.1.1",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "i18n-js": "^3.3.0",
    "isomorphic-unfetch": "^3.0.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.21",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.2",
    "react-native-animatable": "^1.3.3",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-pager-view": "6.7.1",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-safe-area-view": "^1.1.1",
    "react-native-screens": "~4.10.0",
    "react-native-svg": "15.11.2",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^6.0.0",
    "redux-thunk": "2.3.0",
    "sentry-expo": "~7.0.0",
    "short-number": "^1.0.7",
    "ts-invariant": "^0.7.0"
  },
  "devDependencies": {
    "@types/jest": "23.3.13",
    "@types/jwt-decode": "^2.2.1",
    "@types/react": "~19.0.10",
    "@types/react-native": "~0.63.2",
    "@types/react-test-renderer": "16.8.3",
    "eslint": "^9.0.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "husky": "^9.1.7",
    "jest": "~29.7.0",
    "jest-expo": "~53.0.5",
    "prettier": "^3.5.3",
    "react-test-renderer": "16.8.3",
    "typescript": "~5.8.3"
  },
  "packageManager": "yarn@1.22.22"
}